@{
    ViewBag.Title = "Home";
}

<!-- Begin #carousel-section -->
<section id="carousel-section" class="section-global-wrapper">
    <div class="container-fluid-kamn">
        <div class="row">
            <div id="carousel-1" class="carousel slide">

                <!-- Indicators -->
                <ol class="carousel-indicators visible-lg">
                    <li data-target="#carousel-1" data-slide-to="0" class="active"></li>
                    <li data-target="#carousel-1" data-slide-to="1"></li>
                    <li data-target="#carousel-1" data-slide-to="2"></li>
                </ol>

                <!-- Wrapper for slides -->
                <div class="carousel-inner">
                    <!-- Begin Slide 1 -->
                    <div class="item active">
                        <img src="~/assets/img/slider/Office_IT_Support.jpg" height="400" alt="">
                        <div class="carousel-caption">
                            <h3 class="carousel-title hidden-xs">COMMERCIAL</h3>
                            <p class="carousel-body">We want your business to be our business.</p>
                        </div>
                    </div>
                    <!-- End Slide 1 -->
                    <!-- Begin Slide 2 -->
                    <div class="item">
                        <img src="~/assets/img/slider/b1.jpg" height="400" alt="">
                        <div class="carousel-caption">
                            <h3 class="carousel-title hidden-xs">RESIDENTIAL</h3>
                            <p class="carousel-body">Affordable, quality cleaning services just a call away.</p>
                        </div>
                    </div>
                    <!-- End Slide 2 -->
                    <!-- Begin Slide 3 -->
                    <div class="item">
                        <img src="~/assets/img/slider/shady-oaks-apt-building-2.jpg" height="400" alt="">
                        <div class="carousel-caption">
                            <h3 class="carousel-title hidden-xs">PROPERTY MANAGEMENT</h3>
                            <p class="carousel-body">Helping you to help others faster and more efficiently.</p>
                        </div>
                    </div>
                    <!-- End Slide 3 -->
                </div>

                <!-- Controls -->
                <a class="left carousel-control" href="#carousel-1" data-slide="prev">
                    <span class="glyphicon glyphicon-chevron-left"></span>
                </a>
                <a class="right carousel-control" href="#carousel-1" data-slide="next">
                    <span class="glyphicon glyphicon-chevron-right"></span>
                </a>
            </div>
        </div>
    </div>
</section>
<!-- End #carousel-section -->
<!-- Begin #services-section -->
<section id="services" class="services-section section-global-wrapper">
    <div class="container">
        <div class="row">
            <div class="services-header">
                <h3 class="services-header-title">We're Here For You.</h3>
                <p class="services-header-body"><em> A brief overview of our services... </em>  </p><hr>
            </div>
        </div>

        <!-- Begin Services Row 1 -->
        <div class="row services-row services-row-head services-row-1">
            <div class="col-lg-4 col-md-4 col-sm-4 col-xs-12">
                <div class="services-group wow animated fadeInLeft" data-wow-offset="40">
                    <p class="services-icon larger"><span class="fa fa-building-o fa-5x" id="building"></span></p>
                    <h4 class="services-title">Commercial</h4>
                    <p class="services-body">Often times, a business doesn't need to hire an in-house janitorial staff to handle the daily cleaning that's required to maintain it's respectable appearance and safe work environment. Contact us today and let us help you meet your goals.</p>
                    <p class="services-more"><a href="contact.html">Contact Us</a></p>
                </div>
            </div>

            <div class="col-lg-4 col-md-4 col-sm-4 col-xs-12">
                <div class="services-group wow animated zoomIn" data-wow-offset="40">
                    <p class="services-icon"><i class="fa fa-home fa-5x"></i></p>
                    <h4 class="services-title">Residential</h4>
                    <p class="services-body">Life can be extremely busy and not everyone has the time to stay on top of housework. If you're ready for a break from one of lifes responsibilities, give us a call and let us help you to make your home a happier place to be.</p>
                    <p class="services-more"><a href="contact.html">Contact Us</a></p>
                </div>
            </div>

            <div class="col-lg-4 col-md-4 col-sm-4 col-xs-12">
                <div class="services-group wow animated fadeInRight" data-wow-offset="40">
                    <p class="services-icon"><i class="fa fa-briefcase fa-5x" id="briefcase"></i></p>
                    <h4 class="services-title">Property Management</h4>
                    <p class="services-body">At Davie Cleaning Service, we understand the principles of a successful business and we know that in order for your property management business to thrive, you need your properties to be in top-notch shape as quickly as possible. We can help with that.</p>
                    <p class="services-more"><a href="contact.html">Contact Us</a></p>
                </div>
            </div>
        </div>
